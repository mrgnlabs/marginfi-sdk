{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "declarationMap": true,
    "allowJs": true,
    "outDir": "./dist",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "downlevelIteration": true
  },
  "include": ["ts/client/src/*.ts"],
  "exclude": ["node_modules"]
}
